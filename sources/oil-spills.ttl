osnm:1340 a owl:NamedIndividual , osnm:OilSpill ;
	osnm:hasLatitude "54.0933" ;
	osnm:hasLongitude "-133.77" ;
	osnm:hasNote "On the afternoon of January 15, 1995, the Canadian Coast Guard received a report that the F/V Alaskan Star, a 73-foot, steel-hulled vessel had capsized and the four- man crew was abandoning the vessel" ;
	osnm:hasSize "90.84984" ;
